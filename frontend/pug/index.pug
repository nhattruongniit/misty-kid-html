extends ./components/layout.pug

block title
    - var titlePage = "Misty - Home"

block content
    // section banner
    .c-banner
        .c-banner__waves
        .c-banner__mouse
            img(src="./images/img-mouse.png", title="Mouse", alt="Mouse")
        .c-banner__slider.js-banner
            .c-banner__item
                img.c-banner__people(src="./images/home/img-people.png", title="People", alt="People")
                .c-banner__bg.c-banner__bg--img1
                .c-banner__caption.c-banner__caption--middle
                    .c-banner__effect.c-banner__title Fashion Boy
                    h1.c-banner__effect.c-banner__heading AUTUMN WINTER
                    .c-banner__effect.c-banner__lead 2017/18
                    .c-banner__button
                        a(href="#").c-button.c-button__white Shop Now
            .c-banner__item
                .c-banner__bg.c-banner__bg--img2
                .c-banner__caption.c-banner__caption--bottom
                    .c-banner__effect.c-banner__title.c-banner__title--secondary Welcome To StyleShop
                    h1.c-banner__effect.c-banner__heading.c-banner__heading--secondary FLASH SALE OFF 
                        span.c-banner__discount 70%
                    .c-banner__button
                        a(href="#").c-button.c-button__white Shop Now
            .c-banner__item
                .c-banner__bg.c-banner__bg--img3
                .c-banner__caption.c-banner__caption--right
                    .c-banner__effect.c-banner__title.c-banner__title--season SALE 50% OFF
                    h1.c-banner__effect.c-banner__heading.c-banner__heading--season END OF SEASON
                    .c-banner__button
                        a(href="#").c-button.c-button__white Shop Now
    // section special
    .c-special
        .o-container.js-special
            .c-special__item
                .c-special__img
                    img(src="./images/img-discount.jpg", title="Product", alt="Product")
                .c-special__description
                    .c-special__title Special Offers
                    h2.c-special__name Deal of the week
                    .c-special__rating
                        .o-rating
                            i.fa.fa-star
                            i.fa.fa-star
                            i.fa.fa-star
                            i.fa.fa-star
                            i.fa.fa-star-o
                        .c-special__rating__id Item #248LS
                    .c-special__hr
                    .c-special__content What you wear is how you present yourself to the world, especially today, when human contacts are so quick. Fashion is instant language.
                    .c-special__money
                        .c-price
                            span.c-price__fix $45.00
                            del.c-price__off $100.00
                    .o-clock.js-clock
                        .o-clock__item
                            .o-clock__times
                                .o-clock__number.js-clock__days
                                .o-clock__text Days
                        .o-clock__item
                            .o-clock__times
                                .o-clock__number.js-clock__hours
                                .o-clock__text Hrs
                        .o-clock__item
                            .o-clock__times
                                .o-clock__number.js-clock__mins
                                .o-clock__text Mins
                        .o-clock__item
                            .o-clock__times
                                .o-clock__number.js-clock__secs
                                .o-clock__text Secs

            .c-special__item
                .c-special__img
                    img(src="./images/img-discount2.jpg", title="Product", alt="Product")
                .c-special__description
                    .c-special__title Special Offers
                    h2.c-special__name Deal of the week
                    .c-special__rating
                        .o-rating
                            i.fa.fa-star
                            i.fa.fa-star
                            i.fa.fa-star
                            i.fa.fa-star
                            i.fa.fa-star-o
                        .c-special__rating__id Item #248LS
                    hr.c-special__hr
                    .c-special__content What you wear is how you present yourself to the world, especially today, when human contacts are so quick. Fashion is instant language.
                    .c-special__money
                        .c-price
                            span.c-price__fix $35.00
                            del.c-price__off $350.00
                    .o-clock.js-clock
                        .o-clock__item
                            .o-clock__times
                                .o-clock__number.js-clock__days
                                .o-clock__text Days
                        .o-clock__item
                            .o-clock__times
                                .o-clock__number.js-clock__hours
                                .o-clock__text Hrs
                        .o-clock__item
                            .o-clock__times
                                .o-clock__number.js-clock__mins
                                .o-clock__text Mins
                        .o-clock__item
                            .o-clock__times
                                .o-clock__number.js-clock__secs
                                .o-clock__text Secs

